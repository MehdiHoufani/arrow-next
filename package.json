{
  "name": "website-arrow",
  "version": "1.0.0",
  "description": "site vitrine",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MehdiHoufani/bootstrap-next.git"
  },
  "engines": {
    "node": "10.15.3",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "prestart": "npm run build",
    "clean-cache": "rm -rf node_modules/.cache",
    "clean:build": "rm -rf build && rm -rf app/out",
    "build:next": "cross-env NODE_ENV=production next build app",
    "test": "test",
    "dev": "cross-env NODE_ENV=development && npm cache clean --force && node server/main.js",
    "build": "next build app && next export app",
    "start": "cross-env NODE_ENV=production && node server/main.js",
    "start:optim": "cross-env NODE_ENV=production && node --optimize_for_size --max_old_space_size=460 --gc_interval=100 server/main.js"
  },
  "author": "mehdi houfani",
  "license": "ISC",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "core-js": "^2.6.9",
    "express": "^4.17.1",
    "lodash.range": "^3.2.0",
    "mobile-detect": "^1.4.3",
    "next": "^7.0.3",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-md": "^1.12.3",
    "react-motion": "^0.5.2",
    "react-responsive-carousel": "^3.1.47",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.3.0",
    "acorn-jsx": "^5.0.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^3.0.1",
    "postcss-css-variables": "^0.11.0",
    "prettier": "1.17.1"
  }
}
